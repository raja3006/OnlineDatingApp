<div class="container">
    
<div class="container-fluid">
 {{#each singles}}
 <hr class="black">
{{#if online}}
<a href="/userProfile/{{_id}}">
<p class="green">{{fullname}}</p>
<div class="row ">
    <div class="col">
        <img src="img/acc.jpg" class="img-fluid" width="64px">
        <br>

        <h4 class="singles-icon">
            <a href="/userProfile/{{_id}}">
        <button class="btn btn-outline-primary"><i class="fa fa-address-book"></i></button>
        </a>

        <a href="/startChat/{{_id}}">
       <button class="btn btn-outline-info">
           <i class="fa fa-commenting"></i> 
        </button>
        </a>

        <a href="/sendSmile/{{_id}}">
         <button class="btn btn-outline-danger">
             <i class="fa fa-smile-o"></i>
         </button>
        </a>

        <i class="fa fa-circle green right">ONLINE</i>
        </h4>
        
    </div>
</div>
</a>

{{else}}
<a href="/userProfile/{{_id}}">
  <div class="row ">
    <div class="col">
        <img src="img/acc.jpg" class="img-fluid" width="64px">
        <br>

        <h4 class="singles-icon">
            <a href="/userProfile/{{_id}}">
        <button class="btn btn-outline-primary"><i class="fa fa-address-book"></i></button>
        </a>

        <a href="/startChat/{{_id}}">
        <button class="btn btn-outline-info">
           <i class="fa fa-commenting"></i> 
        </button>
        </a>

         <a href="/sendSmile/{{_id}}">
         <button class="btn btn-outline-danger">
             <i class="fa fa-smile-o"></i>
         </button>
        </a>

        <i class="fa fa-circle red right">OFFLINE</i>
        </h4>

        
    </div>
</div>
</a>
{{/if}}
{{else}}
<p>No user Found</p>

{{/each}}
</div>

</div>